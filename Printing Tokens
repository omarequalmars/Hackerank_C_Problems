#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#define len 10

int main() {

char c[1000];
    scanf("%s",c);
    int i,j,check=0;  
       for(i=0;i<=9;i++)
        {
       for(j=0;j<strlen(c);j++)
       {
           if(48+i==c[j])
                check++;
       }
       printf("%d",check);
       printf(" ");
       check=0;    
   }
    return 0;
}
